<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="./xsl/analysisparam.xsl" ?>
<analysis>
  <name>Norm</name>
  <comment>2014.12.12 ichinohe</comment>

  <detectors>
    <detector>
      <name>CdTePad</name>
      <type>2</type>
      <noiselevel>
        <param00>2.0</param00> <!--noiseConstantPside-->
        <param01>0.0</param01>
        <param02>0.01</param02> <!--noiseRatioPside-->
        <param10>2.0</param10>  <!--noiseConstantNside-->
        <param11>0.0</param11>
        <param12>0.01</param12> <!--noiseRatioNside-->
      </noiselevel>
      <analysis>
        <mode>1</mode>
        <threshold>10.0</threshold>
        <threshold_cathode>10.0</threshold_cathode>
        <threshold_anode>10.0</threshold_anode>
      </analysis>
    </detector>
  </detectors>

</analysis>

